<tal:industries repeat="industry registered_industries">
    #${industry.id} polygon {
        <tal:lifetype tal:switch="industry.default_industry_properties.life_type">
            <tal:block tal:case="'IND_LIFE_TYPE_EXTRACTIVE'">
                fill: #2A7234;
                stroke: #2A7234;
            </tal:block>
            <tal:block tal:case="'IND_LIFE_TYPE_ORGANIC'">
                fill: #2A7234;
                stroke: #2A7234;
            </tal:block>
            <tal:block tal:case="'IND_LIFE_TYPE_PROCESSING'">
                fill: #C13424;
                stroke: #C13424;
            </tal:block>
            <tal:block tal:case="'IND_LIFE_TYPE_BLACK_HOLE'">
                fill: #00698C;
                stroke: #00698C;
            </tal:block>
        </tal:lifetype>
    }
    #${industry.id} text {
        <tal:lifetype tal:switch="industry.default_industry_properties.life_type">
            <tal:block tal:case="'IND_LIFE_TYPE_EXTRACTIVE'">
                fill: white;
            </tal:block>
            <tal:block tal:case="'IND_LIFE_TYPE_ORGANIC'">
                fill: white;
            </tal:block>
            <tal:block tal:case="'IND_LIFE_TYPE_PROCESSING'">
                fill: white;
            </tal:block>
            <tal:block tal:case="'IND_LIFE_TYPE_BLACK_HOLE'">
                fill: #fff;
            </tal:block>
        </tal:lifetype>
    }
</tal:industries>

<tal:cargos repeat="cargo registered_cargos">
    <tal:cargo condition="cargo.id not in doc_helper.get_cargoflow_banned_cargos()">
        #${cargo.id} ellipse {
            fill: #D8DBE2;
            stroke: #D8DBE2;
            <tal:supplies condition="cargo.id in doc_helper.get_cargoflow_supply_cargos()">
                fill: #E0E2DB;
                stroke: #E0E2DB;
            </tal:supplies>
        }
    <tal:cargo>
</tal:cargos>
