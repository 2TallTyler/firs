<tal:industries repeat="industry registered_industries">
    #${industry.id} polygon {
        <tal:lifetype tal:switch="industry.default_industry_properties.life_type">
            <tal:primary tal:case="'IND_LIFE_TYPE_EXTRACTIVE'">
                fill: #00560D;
                stroke: #00560D;
            </tal:primary>
            <tal:primary tal:case="'IND_LIFE_TYPE_ORGANIC'">
                fill: #00560D;
                stroke: #00560D;
            </tal:primary>
            <tal:primary tal:case="'IND_LIFE_TYPE_PROCESSING'">
                fill: #1c448c;
                stroke: #1c448c;
            </tal:primary>
            <tal:primary tal:case="'IND_LIFE_TYPE_BLACK_HOLE'">
                fill: #882034;
                stroke: #882034;
            </tal:primary>
        </tal:lifetype>
    }
    #${industry.id} text {
        <tal:lifetype tal:switch="industry.default_industry_properties.life_type">
            <tal:primary tal:case="'IND_LIFE_TYPE_EXTRACTIVE'">
                fill: white;
            </tal:primary>
            <tal:primary tal:case="'IND_LIFE_TYPE_ORGANIC'">
                fill: white;
            </tal:primary>
            <tal:primary tal:case="'IND_LIFE_TYPE_PROCESSING'">
                fill: white;
            </tal:primary>
            <tal:primary tal:case="'IND_LIFE_TYPE_BLACK_HOLE'">
                fill: #fff;
            </tal:primary>
        </tal:lifetype>
    }
</tal:industries>

<tal:cargos repeat="cargo registered_cargos">
    <tal:cargo condition="cargo.id not in doc_helper.get_cargoflow_banned_cargos()">
        #${cargo.id} ellipse {
            fill: #ccd0dc;
            stroke: #ccd0dc;
            <tal:supplies condition="cargo.id in doc_helper.get_cargoflow_supply_cargos()">
                fill: #CFE8EF;
                stroke: #CFE8EF;
            </tal:supplies>
        }
    <tal:cargo>
</tal:cargos>
